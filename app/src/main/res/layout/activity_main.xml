<?xml version="1.0" encoding="utf-8"?>
<androidx.percentlayout.widget.PercentFrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <tech.huqi.smartopencv.core.preview.CameraPreview
            android:id="@+id/javaCameraView"
            android:layout_width="match_parent"
            android:layout_height="250dp"
            app:camera_id="any"
            app:show_fps="false" />
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#000000"
            android:paddingTop="5dp">
            <com.beijingepidial.ccpvirus.LevelView
                android:id="@+id/gv_hv"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_centerHorizontal="true"
                app:bubbleColor="#e03524"
                app:bubbleRadius="10dp"
                app:bubbleRuleColor="#ffffff"
                app:bubbleRuleRadius="12dp"
                app:bubbleRuleWidth="1dp"
                app:limitCircleWidth="3dp"
                app:limitColor="#e03524"
                app:limitRadius="20dp"
                app:horizontalColor="#00ff00"/>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_alignParentBottom="true"
                android:padding="5dp">

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.5">

                    <TextView
                        android:id="@+id/tvv_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:gravity="center_horizontal"
                        android:text="vert"
                        android:textColor="#fff"
                        android:textSize="20sp" />

                    <TextView
                        android:id="@+id/tvl_vertical"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tvv_vertical"
                        android:layout_centerHorizontal="true"
                        android:text="@string/vertical"
                        android:gravity="center_horizontal"
                        android:textColor="@android:color/darker_gray"
                        android:textSize="10sp" />
                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_weight="0.5">
                    <TextView
                        android:id="@+id/tvv_horz"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:gravity="center_horizontal"
                        android:text="horiz"
                        android:textColor="#fff"
                        android:textSize="20sp" />
                    <TextView
                        android:id="@+id/tvl_horz"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/tvv_horz"
                        android:layout_centerHorizontal="true"
                        android:gravity="center_horizontal"
                        android:text="@string/horizontal"
                        android:textColor="@android:color/darker_gray"
                        android:textSize="10sp" />
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <CheckBox
                android:id="@+id/ckAuto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/automatic"/>
            <CheckBox
                android:id="@+id/ckManual"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/manual"/>
        </LinearLayout>

        <androidx.percentlayout.widget.PercentRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <FrameLayout
                android:layout_height="wrap_content"
                android:layout_width="match_parent">
               <com.duan.colorpicker.ColorPickerView
                    android:layout_width="match_parent"
                    android:layout_height="10dp"
                    app:indicatorEnable="false"
                    app:indicatorColor="#fff"
                    app:orientation="horizontal" />
                <com.innovattic.rangeseekbar.RangeSeekBar
                    android:id="@+id/skA"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:rsb_minRange="2"
                    app:rsb_sidePadding="10dp"
                    app:rsb_initialMaxThumbValue="20"
                    app:rsb_max="180"/>
            </FrameLayout>

        </androidx.percentlayout.widget.PercentRelativeLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <androidx.percentlayout.widget.PercentRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="100%"
                    android:layout_width="0dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvRed2Orange"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#FF6100"
                        android:text="@string/red2orange"/>
                    <SeekBar
                        android:id="@+id/sbROthold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:progress="6"
                        android:min="1"
                        android:max="25"
                        android:thumb="@drawable/shape_red_circle"/>
                </LinearLayout>

            </androidx.percentlayout.widget.PercentRelativeLayout>
            <androidx.percentlayout.widget.PercentRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:layout_height="wrap_content"
                    app:layout_widthPercent="50%"
                    android:layout_width="0dp"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvOrange2Yellow"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#FF6100"
                        android:text="@string/orange2yellow"/>
                    <SeekBar
                        android:id="@+id/sbOYthold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:progress="20"
                        android:min="11"
                        android:max="34"
                        android:thumb="@drawable/shape_yellow_circle"/>
                </LinearLayout>
                <LinearLayout
                    app:layout_widthPercent="50%"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvYellow2Green"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#FF6100"
                        android:text="@string/yellow2green"/>
                    <SeekBar
                        android:id="@+id/sbYGthold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:progress="40"
                        android:min="35"
                        android:max="77"
                        android:thumb="@drawable/shape_yellow_circle"/>
                </LinearLayout>
            </androidx.percentlayout.widget.PercentRelativeLayout>
            <androidx.percentlayout.widget.PercentRelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <LinearLayout
                    app:layout_widthPercent="50%"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentLeft="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvCyan2Blue"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#FF6100"
                        android:text="@string/cyan2blue"/>
                    <SeekBar
                        android:id="@+id/sbCBthold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:progress="90"
                        android:min="78"
                        android:max="124"
                        android:thumb="@drawable/shape_blue_circle"/>
                </LinearLayout>
                <LinearLayout
                    app:layout_widthPercent="50%"
                    android:layout_height="wrap_content"
                    android:layout_width="0dp"
                    android:layout_alignParentTop="true"
                    android:layout_alignParentRight="true"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tvBlue2Purple"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:textColor="#8A2BE2"
                        android:text="@string/blue2purple"/>
                    <SeekBar
                        android:id="@+id/sbBPthold"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:progress="120"
                        android:min="125"
                        android:max="180"
                        android:thumb="@drawable/shape_blue_circle"/>
                </LinearLayout>
            </androidx.percentlayout.widget.PercentRelativeLayout>

        </LinearLayout>
        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical">
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Rectancle_size"/>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="90dp">
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:id="@+id/RlMoving"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        app:layout_widthPercent="30%"
                        app:layout_heightPercent="100%">
                        <Button
                            android:id="@+id/btnLeft"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="50%"
                            app:layout_heightPercent="50%"
                            android:text="←" />
                        <Button
                            android:id="@+id/btnRight"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentRight="true"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="50%"
                            app:layout_heightPercent="50%"
                            android:text="→" />
                        <Button
                            android:id="@+id/btnUp"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentLeft="true"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="50%"
                            app:layout_heightPercent="50%"
                            android:text="↑" />
                        <Button
                            android:id="@+id/btnDown"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentRight="true"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="50%"
                            app:layout_heightPercent="50%"
                            android:text="↓" />
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:id="@+id/RLzoom"
                        android:layout_toRightOf="@+id/RlMoving"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="45%"
                        app:layout_heightPercent="100%">
                        <Button
                            android:id="@+id/btnHeightMax"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="35%"
                            app:layout_heightPercent="50%"
                            android:text="↕" />
                        <Button
                            android:id="@+id/btnWidthMax"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="35%"
                            app:layout_heightPercent="50%"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentLeft="true"
                            android:text="↔" />
                        <Button
                            android:id="@+id/btnWidthMin"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentBottom="true"
                            app:layout_widthPercent="65%"
                            app:layout_heightPercent="50%"
                            android:text="width-min" />
                        <Button
                            android:id="@+id/btnHeightMin"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_alignParentRight="true"
                            android:layout_alignParentTop="true"
                            app:layout_widthPercent="65%"
                            app:layout_heightPercent="50%"
                            android:text="height-min" />
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:layout_toRightOf="@+id/RLzoom"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="25%"
                        app:layout_heightPercent="100%">
                        <Button
                            android:id="@+id/btnCatchColor"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="100%"
                            app:layout_heightPercent="50%"
                            android:enabled="false"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:text="@string/catch_color" />
                        <Button
                            android:id="@+id/btnReTake"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="100%"
                            app:layout_heightPercent="50%"
                            android:layout_alignParentBottom="true"
                            android:layout_alignParentLeft="true"
                            android:text="@string/retake" />
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                </androidx.percentlayout.widget.PercentRelativeLayout>

                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:id="@+id/LayoutColorPal"
                    android:layout_width="match_parent"
                    android:layout_height="180dp"
                    android:visibility="gone"
                    android:layout_weight="1"
                    android:gravity="center">
                    <TextView
                        android:id="@+id/tvColorPal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:text="@string/color_palette"/>
                    <SurfaceView
                        android:id="@+id/svColorPlate"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_heightPercent="100%"
                        android:layout_below="@+id/tvColorPal"
                        app:layout_widthPercent="98%" />

                </androidx.percentlayout.widget.PercentRelativeLayout>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:text="Set cell position"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="vertical"
                        android:gravity="center">
                        <Button
                            android:id="@+id/btnCellUp"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="↑"/>
                    </LinearLayout>
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:layout_height="40dp"
                        android:layout_width="match_parent">
                        <Button
                            android:id="@+id/btnCellLeft"
                            android:layout_height="0dp"
                            android:layout_width="0dp"
                            app:layout_widthPercent="20%"
                            app:layout_heightPercent="100%"
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:text="←"/>
                        <Spinner
                            android:id="@+id/spCellRow"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="30%"
                            app:layout_heightPercent="100%"
                            android:layout_toRightOf="@+id/btnCellLeft"
                            android:gravity="center"/>
                        <Spinner
                            android:id="@+id/spCellCol"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="30%"
                            app:layout_heightPercent="100%"
                            android:layout_toRightOf="@+id/spCellRow"
                            android:gravity="center"/>
                        <Button
                            android:id="@+id/btnCellRight"
                            android:layout_height="0dp"
                            android:layout_width="0dp"
                            app:layout_heightPercent="100%"
                            app:layout_widthPercent="20%"
                            android:layout_toRightOf="@+id/spCellCol"
                            android:text="→"/>
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp"
                        android:orientation="vertical"
                        android:gravity="center">
                        <Button
                            android:id="@+id/btnCellDown"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:text="↓"/>
                    </LinearLayout>
                </LinearLayout>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp">
                    <TextView
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        android:text="@string/Setting_Rows" />
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:text="@string/Setting_Column"/>
                </androidx.percentlayout.widget.PercentRelativeLayout>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp">
                        <Button
                            android:id="@+id/btnRowMin"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Min"/>
                        <EditText
                            android:id="@+id/edtRow"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_toRightOf="@+id/btnRowMin"
                            app:layout_widthPercent="13%"
                            app:layout_heightPercent="100%"
                            android:gravity="center"
                            android:enabled="false"
                            android:text="H" />
                        <Button
                            android:id="@+id/btnRowMax"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_toRightOf="@+id/edtRow"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Max"/>
                        <Button
                            android:id="@+id/btnColMin"
                            android:layout_toRightOf="@+id/btnRowMax"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Min"/>
                        <EditText
                            android:id="@+id/edtCol"
                            android:layout_toRightOf="@+id/btnColMin"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="13%"
                            app:layout_heightPercent="100%"
                            android:gravity="center"
                            android:enabled="false"
                            android:text="12" />
                        <Button
                            android:id="@+id/btnColMax"
                            android:layout_toRightOf="@+id/edtCol"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Max"/>
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                </androidx.percentlayout.widget.PercentRelativeLayout>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="15dp">
                    <TextView
                        android:layout_alignParentLeft="true"
                        android:layout_alignParentTop="true"
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        android:text="@string/x_axis_space" />
                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="0dp"
                        app:layout_widthPercent="50%"
                        app:layout_heightPercent="100%"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:text="@string/circle_yxis_space"/>
                </androidx.percentlayout.widget.PercentRelativeLayout>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="45dp">
                    <androidx.percentlayout.widget.PercentRelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="40dp">
                        <Button
                            android:id="@+id/btnMixRowDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_alignParentLeft="true"
                            android:layout_alignParentTop="true"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Min"/>
                        <EditText
                            android:id="@+id/edtRowDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_toRightOf="@+id/btnMixRowDelta"
                            app:layout_widthPercent="13%"
                            app:layout_heightPercent="100%"
                            android:gravity="center"
                            android:enabled="false"
                            android:text="0" />
                        <Button
                            android:id="@+id/btnMaxRowDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_toRightOf="@+id/edtRowDelta"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Max"/>
                        <Button
                            android:id="@+id/btnMixColDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            android:layout_toRightOf="@+id/btnMaxRowDelta"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Mix">
                        </Button>
                        <EditText
                            android:id="@+id/edtColDelta"
                            android:layout_toRightOf="@+id/btnMixColDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="13%"
                            app:layout_heightPercent="100%"
                            android:gravity="center"
                            android:enabled="false"
                            android:text="0"/>
                        <Button
                            android:id="@+id/btnMaxColDelta"
                            android:layout_toRightOf="@+id/edtColDelta"
                            android:layout_width="0dp"
                            android:layout_height="0dp"
                            app:layout_widthPercent="18.5%"
                            app:layout_heightPercent="100%"
                            android:text="Max">
                        </Button>
                    </androidx.percentlayout.widget.PercentRelativeLayout>
                </androidx.percentlayout.widget.PercentRelativeLayout>
                <androidx.percentlayout.widget.PercentRelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="200dp">
                        <LinearLayout
                            android:layout_alignParentTop="true"
                            android:layout_alignParentLeft="true"
                            android:layout_width="0dp"
                            android:layout_height="200dp"
                            app:layout_widthPercent="50%"
                            android:orientation="vertical">
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/move_row_up_down" />
                            <androidx.percentlayout.widget.PercentRelativeLayout
                                android:layout_width="match_parent"
                                android:layout_height="50dp">
                                <Button
                                    android:id="@+id/btnRowUp"
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    app:layout_widthPercent="25%"
                                    app:layout_heightPercent="100%"
                                    android:layout_alignParentTop="true"
                                    android:layout_alignParentLeft="true"
                                    android:text="↑"/>
                                <Spinner
                                    android:id="@+id/spRowNumUpDown"
                                    android:layout_width="0dp"
                                    android:layout_height="0dp"
                                    app:layout_widthPercent="50%"
                                    app:layout_heightPercent="100%"
                                    android:layout_toRightOf="@+id/btnRowUp"
                                    android:gravity="center"/>
